<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mappers.FlowMapper">
  <resultMap id="BaseResultMap" type="com.xiaomai.supershopowner.entity.Flow">
    <id column="id" jdbcType="INTEGER" property="id" />
    <result column="flow_code" jdbcType="VARCHAR" property="flowCode" />
    <result column="store_code" jdbcType="VARCHAR" property="storeCode" />
    <result column="type" jdbcType="INTEGER" property="type" />
    <result column="amount" jdbcType="DECIMAL" property="amount" />
    <result column="flow_time" jdbcType="DATE" property="flowTime" />
  </resultMap>
  <sql id="Base_Column_List">
    id, flow_code, type, amount, flow_time,store_code
  </sql>
  <select id="findByStoreCode" resultMap="BaseResultMap">
    select 
    <include refid="Base_Column_List" />
    from flow
    where store_code = #{storeCode,jdbcType=INTEGER}
  </select>
  
</mapper>