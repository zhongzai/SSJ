<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "mybatis-3-config.dtd">
<configuration>

	<settings>
		<!-- 禁用缓存，默认是true -->
		<setting name="cacheEnabled" value="false" />
		<!-- 配置默认的执行器，默认是SIMPLE -->
		<setting name="defaultExecutorType" value="SIMPLE" />
		<!-- JDBC type for null values，默认是OTHER -->
		<setting name="jdbcTypeForNull" value="OTHER" />
	</settings>

	<typeAliases>
		<typeAlias alias="article"
			type="com.xiaomai.supershopowner.entity.Article" />
		<typeAlias alias="cust"
			type="com.xiaomai.supershopowner.entity.Cust" />
		<typeAlias alias="daycategory"
			type="com.xiaomai.supershopowner.entity.DayCategory" />
		<typeAlias alias="goods"
			type="com.xiaomai.supershopowner.entity.Goods" />
		<typeAlias alias="onecategoryanalyse"
			type="com.xiaomai.supershopowner.entity.OneCategoryAnalyse" />
		<typeAlias alias="picture"
			type="com.xiaomai.supershopowner.entity.Picture" />
		<typeAlias alias="sale"
			type="com.xiaomai.supershopowner.entity.Sale" />
		<typeAlias alias="saleanalyse"
			type="com.xiaomai.supershopowner.entity.SaleAnalyse" />
		<typeAlias alias="storage"
			type="com.xiaomai.supershopowner.entity.Storage" />
		<typeAlias alias="supplementgood"
			type="com.xiaomai.supershopowner.entity.SupplementGood" />
		<typeAlias alias="userlogin"
			type="com.xiaomai.supershopowner.entity.UserLogin" />
		<typeAlias alias="custflow"
			type="com.xiaomai.supershopowner.entity.CustFlow" />
		<typeAlias alias="weeksales"
			type="com.xiaomai.supershopowner.entity.WeekSales" />
		<typeAlias alias="salesTotal"
			type="com.xiaomai.supershopowner.entity.SalesTotalTranfer" />
		<typeAlias alias="vision"
			type="com.xiaomai.supershopowner.entity.Vision" />

	</typeAliases>


	<mappers>
		<mapper resource="mappers/Article.xml" />
		<mapper resource="mappers/Cust.xml" />
		<mapper resource="mappers/DayCategory.xml" />
		<mapper resource="mappers/Goods.xml" />
		<mapper resource="mappers/OneCategoryAnalyse.xml" />
		<mapper resource="mappers/Picture.xml" />
		<mapper resource="mappers/Sale.xml" />
		<mapper resource="mappers/SaleAnalyse.xml" />
		<mapper resource="mappers/Storage.xml" />
		<mapper resource="mappers/SupplementGood.xml" />
		<mapper resource="mappers/UserLogin.xml" />
		<mapper resource="mappers/CustFlow.xml" />
		<mapper resource="mappers/WeekSales.xml" />
		<mapper resource="mappers/SaleTotal.xml" />
		<mapper resource="mappers/Vision.xml" />
	</mappers>
</configuration>